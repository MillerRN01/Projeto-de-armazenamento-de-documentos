# Configurações do Servidor
PORT=5000
NODE_ENV=development

# Configurações do Banco de Dados
DB_HOST=localhost
DB_PORT=5432
DB_NAME=document_management
DB_USER=postgres
DB_PASSWORD=sua_senha_aqui

# Configurações de JWT
JWT_SECRET=seu_secret_key_aqui
